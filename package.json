{
  "name": "repetitivo",
  "version": "0.0.1",
  "description": "A system.",
  "main": "lib/repetitivo.scss",
  "scripts": {
    "build": "node-sass lib/repetitivo.scss build/repetitivo.css",
    "generate:docs": "sassdoc lib/repetitivo",
    "generate:docs:watch": "nodemon -w lib -e scss -x 'npm run generate:docs'",
    "start": "npm run build -- --watch",
    "test": "npm run test:build && npm run test:unit",
    "test:build": "test/build_individually.sh",
    "test:unit": "node_modules/.bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oktavilla/repetitivo.git"
  },
  "author": "Oktavilla",
  "license": "ISC",
  "private": true,
  "bugs": {
    "url": "https://github.com/Oktavilla/repetitivo/issues"
  },
  "homepage": "https://github.com/Oktavilla/repetitivo#readme",
  "devDependencies": {
    "json": "~9.0.3",
    "mocha": "^5.1.1",
    "node-sass": "~4.9.0",
    "nodemon": "^1.17.4",
    "sass-true": "^4.0.0",
    "sassdoc": "^2.5.0"
  }
}
